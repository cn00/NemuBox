[Version]
Signature="$Windows NT$"
Class=USB
ClassGUID={36FC9E60-C465-11CF-8056-444553540000}
provider=%ORACLE%
;edit-DriverVer=08/26/2008,2.00.0000
;cat CatalogFile=NemuUSB.cat

[SourceDisksNames]
1=%Disk_Description%,,,

[SourceDisksFiles]
NemuUSB.sys = 1

[Manufacturer]
;x86   %MfgName%=ORACLE
;amd64 %MfgName%=ORACLE, NTamd64

;x86   [ORACLE]
;amd64 [ORACLE.NTamd64]
%USB\VID_80EE&PID_CAFE.DeviceDesc%=NemuUSB.Dev, USB\VID_80EE&PID_CAFE

[DestinationDirs]
NemuUSB.Files.Ext = 10,System32\Drivers

[NemuUSB.Dev.NT]
CopyFiles=NemuUSB.Files.Ext

[NemuUSB.Dev.NT.Services]
Addservice = NemuUSB, 0x00000002, NemuUSB.AddService

[NemuUSB.AddService]
DisplayName    = %NemuUSB.SvcDesc%
ServiceType    = 1                  ; SERVICE_KERNEL_DRIVER
StartType      = 3                  ; SERVICE_DEMAND_START
ErrorControl   = 1                  ; SERVICE_ERROR_NORMAL
ServiceBinary  = %10%\System32\Drivers\NemuUSB.sys
AddReg         = NemuUSB.AddReg
LoadOrderGroup = Base

[NemuUSB.AddReg]
HKR,,DevLoader,,*ntkern
HKR,,NTMPDriver,,NemuUSB.sys

[NemuUSB.Files.Ext]
NemuUSB.sys

;---------------------------------------------------------------;

[Strings]
ORACLE="Oracle Corporation"
MfgName="Oracle Corporation"
Disk_Description="NemuUSB Installation Disk"
USB\VID_80EE&PID_CAFE.DeviceDesc="VirtualBox USB"
NemuUSB.SvcDesc="VirtualBox USB"
