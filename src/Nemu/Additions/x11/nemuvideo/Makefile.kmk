# Check the undefined symbols in the X.Org modules against lists of allowed
# symbols.  Not very elegant, but it will catch problems early.

ifdef NEMU_WITH_TESTCASES
# ifndef NEMU_ONLY_ADDITIONS
 ifndef NEMU_USE_SYSTEM_XORG_HEADERS
  if1of ($(KBUILD_TARGET),linux solaris)
   ifeq ($(KBUILD_HOST_ARCH),$(KBUILD_TARGET_ARCH))
    ifndef NEMU_ONLY_SDK
     NEMUVIDEO_SRC_PATH := $(PATH_SUB_CURRENT)

     ifeq ($(KBUILD_TARGET),linux)
      TESTING += $(nemuvideo_drv_0_OUTDIR)/tstnemuvideo68.run
      OTHERS += $(nemuvideo_drv_0_OUTDIR)/tstnemuvideo68.run
$$(nemuvideo_drv_0_OUTDIR)/tstnemuvideo68.run: $$(nemuvideo_drv_1_STAGE_TARGET)
	$(QUIET)$(call MSG_L1,Checking for unresolved symbols in $<)
	$(QUIET)$(ASH) $(PATH_ROOT)/src/bldprogs/checkUndefined.sh $(KBUILD_HOST)  \
	    $(nemuvideo_drv_1_STAGE_TARGET) --static $(NEMUVIDEO_SRC_PATH)/../undefined_xfree86 $(NEMUVIDEO_SRC_PATH)/../undefined_xfree86_modules
	$(QUIET)$(APPEND) -t "$@" "done"
     endif

##
# Using the extra expansion to replace $(ver) before eval, thus everything
# else needs escaped dollars.
     define def_nemuvideo_test
      TESTING += $$(nemuvideo_drv$(ver)_0_OUTDIR)/tstnemuvideo$(ver).run
      OTHERS  += $$(nemuvideo_drv$(ver)_0_OUTDIR)/tstnemuvideo$(ver).run
      $$$$(nemuvideo_drv$(ver)_0_OUTDIR)/tstnemuvideo$(ver).run: $$$$(nemuvideo_drv$(ver)_1_STAGE_TARGET)
	$$(QUIET)$$(call MSG_L1,Checking for unresolved symbols in $$<)
	$$(QUIET)$$(ASH) $$(PATH_ROOT)/src/bldprogs/checkUndefined.sh $$(KBUILD_HOST)  \
	        $$(nemuvideo_drv$(ver)_1_STAGE_TARGET) $$(NEMUVIDEO_SRC_PATH)/../undefined_xfree86 $(NEMUVIDEO_SRC_PATH)/../undefined_xfree86_modules $$(NEMUVIDEO_SRC_PATH)/../undefined_xorg
	$$(QUIET)$$(APPEND) -t "$$@" "done"
    endef

    $(foreach ver, _70 _71 _13 _14 _15 _16 _17 _18 _19 _110 _111 _112 _113 _114 _115 _116 _117 _118, $(eval $(def_nemuvideo_test)))

    endif # ! NEMU_ONLY_SDK
   endif # eq ($(KBUILD_HOST_ARCH),$(KBUILD_TARGET_ARCH))
  endif # 1of ($(KBUILD_TARGET),linux solaris)
 endif # ! NEMU_USE_SYSTEM_XORG_HEADERS
# endif # ! NEMU_ONLY_ADDITIONS
endif # NEMU_WITH_TESTCASES

